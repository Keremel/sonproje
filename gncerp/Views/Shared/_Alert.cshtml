@model gncerp.Utility.ResultV
@{
    ViewData["Title"] = "CompanyRegister";
    Layout = "";
}

<html lang="tr">
<head>
    <meta charset="utf-8" />
    <title>Gençpa | Bilgilendirme</title>
    <meta name="description" content="<%=html_page_desc%>">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="~/lib/css/demo1/pages/login/login-3.css" rel="stylesheet" type="text/css" />
    <link href="~/lib/css/demo10/style.bundle.css" rel="stylesheet" type="text/css" />


    <link href="~/lib/media/logos/favicon.png" rel="shortcut icon" />


</head>
<body class="kt-quick-panel--right kt-demo-panel--right kt-offcanvas-panel--right kt-header--fixed kt-header-mobile--fixed kt-subheader--enabled kt-subheader--fixed kt-subheader--solid kt-aside--enabled kt-aside--fixed kt-page--loading">
    <div class="kt-grid kt-grid--ver kt-grid--root">
        <div class="kt-grid kt-grid--hor kt-grid--root kt-login kt-login--v3 kt-login--signin" id="kt_login">
            <div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor" style="background-image: url(~/lib/media/bg/bg-1.jpg);">
                <div class="kt-grid__item kt-grid__item--fluid kt-login__wrapper" style="padding:1.3rem; margin:0 auto 0.5rem auto">
                    <div class="kt-login__container " style="padding:25px; background-color:#fff; background-image:url(~/lib/media/bg/bg-3.jpg);">
                        <div class="kt-login__logo"><img style="height:320px" src="~/lib/media/logos/yfa_text.png"></a></div>



                    </div>
                </div>
            </div>
        </div>


    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <partial name="_ValidationScriptsPartial" />
    <script src="~/lib/vendors/general/bootstrap/dist/js/bootstrap.min.js" type="text/javascript"></script>

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>


    <script>


        $(document).ready(function () {
                     Swal.fire({
                     title: '@Model.title',
                     text: '@Model.text',
                     icon: '@Model.icon',
                    showCancelButton: false,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Tamam'
                }).then((result) => {
                      window.location.assign("@Model.redirectingUrl");

                })


            setInterval(function () {
                window.location.assign("@Model.redirectingUrl");


            },@Model.redirectingTimeout);
        });

    </script>


</body>




</html>








